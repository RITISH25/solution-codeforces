#include <iostream>
#include <vector>
#include <algorithm>
#include <set>
using namespace std;
set<char> val;
int main(){
int n;
cin>>n;
while(n--){
        char c;
        cin>>c;
        val.insert(tolower(c));
}
if(val.size()==26)
    cout<<"YES"<<endl;
    else
        cout<<"NO"<<endl;
val.clear();
}
