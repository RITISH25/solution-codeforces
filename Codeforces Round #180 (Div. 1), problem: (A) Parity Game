#include <bits/stdc++.h>
using namespace std;
int main(){
	
	string s,t;
	cin>>s>>t;
	int s1=0;int t1=0;
	for(int i=0;i<s.size();i++){
		if(s[i]=='1')s1++;
	}
	for(int i=0;i<t.size();i++){
		if(t[i]=='1') t1++;
	}
	if(s1+s1%2>=t1){
		cout<<"YES"<<endl;
		return 0;
	}
	else{
		cout<<"NO"<<endl;
		return 0;
	}
}
