#include <bits/stdc++.h>

using namespace std;
//char s[100];
//int a[52];
int main()
{
    int n,t;
    cin>>n>>t;
 string s;
        cin>>s;

        while(t--){
                  string s1=s;
   for(int i=0;i<n;i+=1){
    if(s[i+1]=='G' && s[i]=='B'){
        swap(s1[i],s1[i+1]);

   }
        }
        s=s1;
        }
        cout<<s<<endl;

    return 0;
}
